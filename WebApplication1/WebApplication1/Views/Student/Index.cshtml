@model Calischool.Models.ApplicationUser

@{
    ViewData["Title"] = "RegisterStudent";
}

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container p-3">
     <div class="row pt-4">
         <div class="col-3">
             <h2 class="text-primary">Student Portal</h2>
         </div>
         
         <div class="col-7 text-end">
            <a asp-controller="#" asp-action="#" class="btn btn-primary border border-info">Html</a>
            <a asp-controller="#" asp-action="#" class="btn btn-primary border border-info ">Css</a>
            <a asp-controller="#" asp-action="#" class="btn btn-primary border border-info ">Bootstrap</a>
            <a asp-controller="#" asp-action="#" class="btn btn-primary border border-info">Javascript</a>
            <a asp-controller="#" asp-action="#" class="btn btn-primary border border-info ">Jquery</a>
            <a asp-controller="#" asp-action="#" class="btn btn-primary border border-info ">C#</a>
            <a asp-controller="#" asp-action="#" class="btn btn-primary border border-info">Asp.Net MCV</a>
         </div>
          <div class="col-2 text-end">
             <a asp-controller="Student" asp-action="Edit" class="btn btn-primary"/>
               <i class="bi bi-plus-circle"></i>
               &nbsp; Edit Details
         </div>

     </div>
</div>
<br /><br />


